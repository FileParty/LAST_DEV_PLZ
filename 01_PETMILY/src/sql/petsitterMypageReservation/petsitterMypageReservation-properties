selectBeforePaymentRerservation=SELECT * FROM PET_RESERVATION P JOIN USER_TABLE U ON(P.USER_ID=U.USER_ID) where pet_sItter_id=?
sitterSettledList = SELECT TO_CHAR(CHECKOUT_DATE, 'yyyy"년 "mm"월"') AS PRDATE, COUNT(*) AS NUMALL, SUM(PR.PRICE) AS PRPRICE, SUM(CEIL(PR.PRICE/9)) AS PRCEIL FROM PET_RESERVATION PR JOIN USER_TABLE UT ON(PR.PET_SETTER_ID=UT.USER_ID) WHERE UT.USER_ID=? GROUP BY TO_CHAR(CHECKOUT_DATE, 'yyyy"년 "mm"월"')
selectPetsitterMypageUserReview=SELECT * FROM USER_TABLE U JOIN REVIEW_USER R ON(U.USER_ID=R.USER_ID) JOIN PET_RESERVATION P ON(R.RESERVATION_CODE=P.RESERVATION_CODE) WHERE R.USER_ID=?
